{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-console": 1,

    "indent": [ "error", 2 ],
    "no-extra-parens": 2,
    "no-trailing-spaces": 2,
    "quotes": [ "error", "single" ]
  }
}
